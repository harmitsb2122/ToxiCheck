{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": "ToxiCheck",
    "includePattern": ".js$",
    "excludePattern": "node_modules/|docs"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./jsdoc",
    "recurse": true
  }
}
